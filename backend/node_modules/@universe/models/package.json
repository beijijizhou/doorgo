{
  "name": "@universe/models",
  "version": "3.0.13",
  "main": "esm/src/index.js",
  "description": "Universe data models.",
  "author": "Adam Miller <adam@universe.app>",
  "license": "UNLICENSED",
  "type": "module",
  "private": false,
  "repository": "https://github.com/universe/big-blue",
  "exports": {
    ".": {
      "types": "./esm/src/index.d.ts",
      "import": "./esm/src/index.js",
      "require": "./esm/src/index.js"
    }
  },
  "files": [
    "esm",
    "schema.gql",
    "!esm/test",
    "!esm/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "build": "rm -rf dist && rm -rf esm && yarn build:typescript && tsc -b tsconfig.cjs.json && tsc -b tsconfig.json && yarn build:dbt && yarn build:python && yarn build:ext && yarn build:docs",
    "build:typescript": "node ./generators/typescript.js",
    "build:dbt": "node ./generators/dbt.js",
    "build:python": "node ./generators/python.js",
    "build:ext": "node ./ext/build/index.mjs",
    "build:docs": "spectaql docs/spectaql.yml",
    "start:docs": "spectaql -D docs/spectaql.yml",
    "pretest": "rm -f ./test.db && yarn run build",
    "test": "mocha esm/test",
    "watch": "watch 'yarn build' './generators' './src' './test' --wait=1"
  },
  "dependencies": {
    "@universe/util": "^1.2.2",
    "node-fetch": "^3.2.3"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@types/handlebars": "^4.0.40",
    "@types/json2csv": "^4.2.0",
    "@types/validator": "^10.11.3",
    "code-block-writer": "^11.0.3",
    "spectaql": "^2.3.0",
    "graphql": "^14.1.1",
    "graphql-code-generator": "^0.17.0",
    "graphql-codegen-typescript-common": "^0.17.0",
    "graphql-codegen-typescript-resolvers": "^0.17.0",
    "graphql-codegen-typescript-server": "^0.17.0",
    "handlebars": "^4.1.0",
    "watch": "^1.0.2"
  }
}